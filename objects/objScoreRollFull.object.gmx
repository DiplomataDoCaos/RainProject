            <string>///Full Run Results!!

draw_set_halign(fa_center);
draw_set_color(c_white);
draw_text(stage_horizon_center,start - 50,"Sua odiséia");
draw_text(levelColumn,start,"Fase")
draw_text(dignityColumn,start,"Dignidade perdida")
draw_text(scoreColumn,start,"Nakacoins")
draw_text(grazeColumn,start,"Roçadas")

// change later to archive access
var place = 1
while place &lt;= listLong
{
    draw_text(levelColumn,start +distance *place, place)
    draw_text(dignityColumn,start +distance *place, global.dignityList[place])
    draw_text(scoreColumn,start +distance *place, global.pointsList[place])
    draw_text(grazeColumn,start +distance *place, global.grazeList[place])
    place = place + 1
}

if check = true
{
    global.dignityTotal += global.dignityList[place-1]
    global.pointsTotal += global.pointsList[place-1]
    global.grazeTotal += global.grazeList[place-1]
    check = false
}


draw_text(levelColumn,start +distance *place,"Total")
draw_text(dignityColumn,start +distance *place, global.dignityTotal)
draw_text(scoreColumn,start +distance *place, global.pointsTotal)
draw_text(grazeColumn,start +distance *place, global.grazeTotal)

draw_set_halign(fa_left);
draw_set_color(c_red);
</string>
